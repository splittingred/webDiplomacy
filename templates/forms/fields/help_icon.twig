<img id="{{ id }}"
     role="button"
     title="{{ title }}"
     data-toggle="{{ toggle }}"
     data-trigger="{{ trigger }}"
     data-html="true"
     data-content="{{ text }}"
     {% if delay %}data-delay="{{ delay }}"{% endif %}
     class="help-btn"
     src="/images/icons/help.png"
     alt="{{ alt }}"
/>